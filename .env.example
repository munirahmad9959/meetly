# =======================================================
# Meetly App Environment Variables (Example)
# =======================================================
# Copy this file to .env and fill in your actual values

# =======================================================
# Firebase Configuration
# =======================================================

# Firebase Web Configuration
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here
FIREBASE_WEB_MESSAGING_SENDER_ID=your_web_messaging_sender_id
FIREBASE_WEB_PROJECT_ID=your_web_project_id
FIREBASE_WEB_AUTH_DOMAIN=your_web_auth_domain
FIREBASE_WEB_STORAGE_BUCKET=your_web_storage_bucket

# Firebase Android Configuration
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here
FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_android_messaging_sender_id
FIREBASE_ANDROID_PROJECT_ID=your_android_project_id
FIREBASE_ANDROID_STORAGE_BUCKET=your_android_storage_bucket

# Firebase iOS Configuration
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_MESSAGING_SENDER_ID=your_ios_messaging_sender_id
FIREBASE_IOS_PROJECT_ID=your_ios_project_id
FIREBASE_IOS_STORAGE_BUCKET=your_ios_storage_bucket
FIREBASE_IOS_BUNDLE_ID=com.example.meetly

# Firebase macOS Configuration
FIREBASE_MACOS_API_KEY=your_macos_api_key_here
FIREBASE_MACOS_APP_ID=your_macos_app_id_here
FIREBASE_MACOS_MESSAGING_SENDER_ID=your_macos_messaging_sender_id
FIREBASE_MACOS_PROJECT_ID=your_macos_project_id
FIREBASE_MACOS_STORAGE_BUCKET=your_macos_storage_bucket
FIREBASE_MACOS_BUNDLE_ID=com.example.meetly

# Firebase Windows Configuration
FIREBASE_WINDOWS_API_KEY=your_windows_api_key_here
FIREBASE_WINDOWS_APP_ID=your_windows_app_id_here
FIREBASE_WINDOWS_MESSAGING_SENDER_ID=your_windows_messaging_sender_id
FIREBASE_WINDOWS_PROJECT_ID=your_windows_project_id
FIREBASE_WINDOWS_AUTH_DOMAIN=your_windows_auth_domain
FIREBASE_WINDOWS_STORAGE_BUCKET=your_windows_storage_bucket
